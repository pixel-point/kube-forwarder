{
  "name": "kube-forwarder",
  "version": "1.5.1",
  "author": "Pixel Point <info@pixelpoint.io>",
  "homepage": "https://kube-forwarder.poxelpoint.io",
  "description": "A tool for managing port forwarding configs for kubernetes clusters",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": "https://github.com/pixel-point/kube-forwarder",
  "scripts": {
    "build": "npm run build:dist && npm run build:target",
    "build:dist": "node .electron-vue/build.js",
    "build:target": "BUILD=$(cat build/.number) electron-builder",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "upload": "./upload.sh",
    "release": "RELEASE=true npm run build -- -- -p always --mac --linux --win",
    "dev": "BUILD=DEV-VERSION node .electron-vue/dev-runner.js",
    "web": "BUILD=WEB-VERSION node .electron-vue/web-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test:cypress:open": "cypress open",
    "test:cypress:onhost": "node .electron-vue/cypress-runner.js --reporter cypress-image-snapshot/reporter",
    "test:cypress": "docker run --rm -ti -w /app -v `pwd`:/app --entrypoint=/bin/bash cypress/included:3.4.0 -c \"npm rebuild node-sass && npm run test:cypress:onhost\"",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "build": {
    "productName": "Kube Forwarder",
    "appId": "com.pixelpoint.kube-forwarder",
    "artifactName": "kube-forwarder.${ext}",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "background": "src/bg.tiff",
      "iconSize": 125,
      "iconTextSize": 13,
      "contents": [
        {
          "x": 501,
          "y": 183,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 163,
          "y": 183,
          "type": "file"
        }
      ],
      "window": {
        "width": 675,
        "height": 433
      }
    },
    "publish": {
      "provider": "github",
      "owner": "pixel-point",
      "repo": "kube-forwarder"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "@kubernetes/client-node": "0.10.2",
    "@sentry/electron": "^0.17.3",
    "@sentry/integrations": "^5.3.1",
    "clone-deep": "^4.0.1",
    "deepmerge": "^4.0.0",
    "dotenv-safe": "^6.1.0",
    "electron-ga": "^1.0.6",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "killable": "^1.0.1",
    "lodash": "^4.17.15",
    "shell-path": "^2.1.0",
    "vue": "^2.6.10",
    "vue-click-outside": "^1.0.7",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.6",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.1",
    "vuex-electron": "^1.0.3",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime-corejs2": "^7.5.5",
    "@sentry/webpack-plugin": "^1.7.0",
    "ajv": "^6.7.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.1.4",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^2.1.1",
    "cypress": "^3.4.0",
    "cypress-image-snapshot": "^3.1.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "11.5.0",
    "electron-builder": "20.41.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
